{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        },
        "applicationName": "Expect Labs mindmeld-js-sdk",
        "openGraph": {
            "type": "website",
            "image": "http://developer.expectlabs.com/public/images/ogImgDevCenter.jpg",
            "site_name": "Expect Labs - MindMeld"
        },
        "meta": {
            "description": "Expect Labs mindmeld-js-sdk Documentation",
            "keyword": "Expect Labs, JavaScript, MindMeld, SDK"
        }
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true,
        "tags": ["examples"]
    }
}